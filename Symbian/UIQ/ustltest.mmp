SOURCE file.cpp
SOURCE memblock.cpp
SOURCE memlink.cpp
SOURCE mstream.cpp
SOURCE sistream.cpp
SOURCE sostream.cpp
SOURCE ualgobase.cpp
SOURCE ubitset.cpp
SOURCE uexception.cpp
SOURCE unew.cpp
SOURCE ustdxept.cpp
SOURCE ustring.cpp
SOURCE SymbianStringSupport.cpp
*/

LIBRARY		estlib.lib euser.lib 
LIBRARY		bafl.lib
STATICLIBRARY ustl.lib
// STATICLIBRARY	ecrt0.lib // doesn't exist in beta 3 - wrote it as SymbianMain

// AIF stuff - make the environment see us:

SOURCEPATH		Reg

START RESOURCE	ustltestUIQ_reg.rss
TARGETPATH		\private\10003a3f\apps
END

START RESOURCE	ustltestUIQ_loc.rss
