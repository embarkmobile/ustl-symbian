VENDORID          0

SYSTEMINCLUDE	\epoc32\include \epoc32\include\libc
SYSTEMINCLUDE   ..\.. 
USERINCLUDE ..\..

SOURCEPATH	..\..\bvt

//SOURCE bvt13.cpp  // Cin, cout, cerr
SOURCE SymbianMain.cpp // Replacement for ecrt0.lib
SOURCE SymbianTest.cpp

/*  For debugging... easier to have all in one package:
SOURCEPATH	..\..

SOURCE cmemlink.cpp
